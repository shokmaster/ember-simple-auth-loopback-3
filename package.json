{
  "name": "ember-simple-auth-loopback-3",
  "version": "2.0.8",
  "description": "Loopback 3 support for ember-simple-auth",
  "keywords": [
    "ember-addon",
    "ember-simple-auth",
    "Loopback",
    "authentication",
    "authorization",
    "auth"
  ],
  "repository": "https://github.com/shokmaster/ember-simple-auth-loopback-3",
  "license": "MIT",
  "author": "Juan Antonio Gomez Benito",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon config tests",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "test:all": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@ember/jquery": "^0.6.0",
    "@ember/optional-features": "^0.7.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.7",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "ember-cli": "~3.7.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-simple-auth": "^1.1.0",
    "ember-sinon": "^3.0.0",
    "ember-source": "^3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^0.2.23",
    "eslint-config-bbva": "^2.0.3",
    "eslint-plugin-ember": "^6.1.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "loader.js": "^4.7.0",
    "merge": ">=1.2.1",
    "qunit-dom": "^0.8.0",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "ember-try"
    ]
  }
}
